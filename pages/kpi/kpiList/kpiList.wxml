<view class='container'>
  <view class='search'>
    <view class='searchBox'>
      <input bindinput='getValue'></input>
    <image mode='aspectFit' bindtap='searchUser' src='http://minipro.arvatocrm.cn/arvato/img/?fileName=1539677268173.png'></image>
    </view>
  </view>
  <view class='kpiLists' wx:for='{{kpiList}}'>
    <view class='list' data-param='{{item.externalId}}' bindtap='toUserDetail'>
      <view class='header'>
        <image class='avator' src='{{item.avatar}}'></image>
      </view>
      <view class='nickName'>{{item.name}}</view>
      <button>查看KPI</button>
    </view>
  </view>
  <view class='userNum'><text>●  </text>总计{{total}}位客户<text>  ●</text></view>
</view>
